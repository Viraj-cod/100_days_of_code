def find(num):
    res = []
    p = 0
    while num > 0:
        k = num % 10
        j = k * 10**p
        res.append(j)
        num = num // 10
        p += 1
    return res
        
def main():
    n = int(input("Enter the number:"))
    print(find(n))

if __name__ == '__main__':
    main()
    
